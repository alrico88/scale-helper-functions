{
  "name": "scale-helper-functions",
  "version": "2.2.0",
  "description": "Helper to create scales",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "exports": {
    "require": "./dist/index.js",
    "import": "./dist/index.mjs"
  },
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "author": "Alberto Rico",
  "repository": {
    "url": "https://github.com/alrico88/scale-helper-functions"
  },
  "keywords": [
    "scales",
    "linear",
    "log",
    "d3",
    "helper"
  ],
  "license": "MIT",
  "dependencies": {
    "d3-scale": "^4.0.2"
  },
  "devDependencies": {
    "@types/d3-scale": "^4.0.3",
    "@typescript-eslint/eslint-plugin": "^6.4.0",
    "@typescript-eslint/parser": "^6.4.0",
    "@vitest/coverage-v8": "^0.34.2",
    "coveralls": "^3.1.1",
    "eslint": "^8.47.0",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.28.0",
    "tsup": "^7.2.0",
    "typedoc": "^0.24.8",
    "typedoc-plugin-markdown": "^3.15.4",
    "typescript": "^5.1.6",
    "vitest": "^0.34.2"
  },
  "scripts": {
    "dev": "pnpm run build -- --watch src",
    "build": "tsup src/index.ts --format cjs,esm --dts --clean",
    "test": "vitest run",
    "coverage": "vitest run --coverage",
    "lint": "eslint --cache --fix --ignore-path .gitignore --ext .ts,.js src",
    "prepublish": "pnpm run build",
    "coveralls": "vitest run --coverage && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js"
  },
  "sideEffects": false
}
